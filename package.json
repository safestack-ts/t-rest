    "changeset:snapshot:publish": "pnpm changeset publish --tag dev --no-git-tag",
    "changeset:snapshot:version": "pnpm changeset version --snapshot dev",
    "pub:dev": "pnpm build && pnpm exec ./scripts/publish-dev-version.sh"
