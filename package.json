  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "test": "pnpm recursive run test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "verify": "tsc  --noEmit",
    "format": "prettier --write \"./**/*.ts\"",
    "changeset:add": "pnpm changeset",
    "changeset:check": "pnpm exec ./scripts/check-changeset-status.sh",
